{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "yarn": "use pnpm please!",
    "npm": "use pnpm please!"
  },
  "engineStrict": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:open-browser": "open http://127.0.0.1:3000",
    "dev:next": "next dev --show-all",
    "build": "next build",
    "start": "next start",
    "tsc": "tsc",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx}' && prettier --write --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,json,gql}'",
    "spell-check": "cspell src/**",
    "compiler-check": "tsc --noEmit",
    "test": "run-p test:*",
    "test:unit": "vitest run",
    "test:e2e": "pnpm exec playwright test",
    "codegen": "graphql-codegen --config codegen.ts",
    "compose:build": "docker compose build --no-cache",
    "compose:up": "docker compose up",
    "reset": "rm -rf node_modules/ && pnpm i"
  },
  "dependencies": {
    "@apollo/client": "3.8.0-alpha.15",
    "@apollo/experimental-nextjs-app-support": "^0.3.2",
    "@chakra-ui/next-js": "^2.1.4",
    "@chakra-ui/react": "^2.7.1",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@types/node": "20.3.3",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "autoprefixer": "10.4.14",
    "eslint": "8.52.0",
    "eslint-config-next": "13.5.6",
    "framer-motion": "^10.12.21",
    "graphql": "^16.7.1",
    "next": "13.5.6",
    "postcss": "8.4.24",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.10.1"
  },
  "volta": {
    "node": "18.16.1",
    "pnpm": "8.6.6"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/client-preset": "4.1.0",
    "@graphql-codegen/introspection": "4.0.0",
    "@playwright/test": "^1.36.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "happy-dom": "^9.20.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.1",
    "typescript": "5.1.6",
    "vitest": "^0.32.4"
  }
}
