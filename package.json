{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev-path": "pathpida --enableStatic --ignorePath .gitignore --watch",
    "build": "pathpida --enableStatic --ignorePath .gitignore && next build",
    "start": "next start",
    "lint": "next lint --dir src",
    "lint:fix": "next lint --dir src --fix",
    "compiler-check": "tsc --noEmit",
    "test": "run-p test:*",
    "test:unit": "vitest run",
    "test:e2e": "pnpm exec playwright test",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@apollo/client": "3.8.0-alpha.15",
    "@apollo/experimental-nextjs-app-support": "^0.3.2",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@types/node": "20.3.3",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "autoprefixer": "10.4.14",
    "eslint": "8.44.0",
    "eslint-config-next": "13.4.8",
    "graphql": "^16.7.1",
    "next": "13.4.8",
    "postcss": "8.4.24",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.2"
  },
  "volta": {
    "node": "18.16.1",
    "pnpm": "8.6.6"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "4.0.1",
    "@graphql-codegen/client-preset": "4.0.1",
    "@graphql-codegen/introspection": "4.0.0",
    "@playwright/test": "^1.35.1",
    "eslint-config-prettier": "^8.8.0",
    "happy-dom": "^9.20.3",
    "npm-run-all": "^4.1.5",
    "pathpida": "^0.20.1",
    "ts-node": "^10.9.1",
    "typescript": "5.1.6",
    "vitest": "^0.32.4"
  }
}
